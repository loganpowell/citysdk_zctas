{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "plugins": ["plugins/markdown"],
    "source": {
        "include": ["src", "README.md"],
        "includePattern": "\\.(jsx|js|ts|tsx)$",
        "excludePattern": "(node_modules)"
    },
    "templates": {
        "referenceTitle": "jsdoc.json docs",
        "disableSort": true,
        "collapse": false
    },
    "opts": {
        "package": "./package.json",
        "destination": ".",
        "encoding": "utf8",
        "recurse": true
    }
}
